{
  "name": "mrtolouei/structify",
  "description": "Structify is a powerful Laravel package that automates the creation of Model, DTO, Service, and Repository files with a single Artisan command.",
  "keywords": [
    "laravel",
    "package",
    "scaffolding",
    "model",
    "dto",
    "service",
    "repository",
    "automation",
    "productivity"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ali Tolouei",
      "email": "mrtolouei.com@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mrtolouei\\Structify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mrtolouei\\Structify\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mrtolouei\\Structify\\StructifyServiceProvider"
      ]
    }
  }
}